diff --git a/Makefile b/Makefile
index 03373bb..9930257 100644
--- a/Makefile
+++ b/Makefile
@@ -1,8 +1,8 @@
 VERSION = 2
 PATCHLEVEL = 6
 SUBLEVEL = 30
-EXTRAVERSION =
-NAME = Man-Eating Seals of Antiquity
+EXTRAVERSION = -coldnew
+NAME = What's this?

 # *DOCUMENTATION*
 # To see a list of typical targets execute "make help"
@@ -333,13 +333,12 @@ CHECK             = sparse
